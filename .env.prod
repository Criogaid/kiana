DRIVER=~fastapi+~httpx+~websockets
COMMAND_START=["/", ""]
APSCHEDULER_CONFIG={ "apscheduler.timezone": "Asia/Shanghai" }
SUPERUSERS=[""]
NICKNAME=[""]

# ONEBOT_ACCESS_TOKEN=

# 金价插件配置
# 金价获取间隔时间（秒），默认600秒
# price_fetch_interval=
# 趋势图展示的时间窗口（小时）, 默认120小时
# chart_window_hours: int =
# 内存中保留的历史数据最大数量，默认86400条
# price_history_limit: int =

# doubili插件配置
# 是否启用B站解析，默认true
# enable_bilibili=
# 是否启用抖音解析，默认true
# enable_douyin=
# 是否启用小红书解析，默认true
# enable_xiaohongshu=
# 小红书 cookie，解析分享卡片必须，直链可以不要
# xiaohongshu_cookie=

# 昵称插件配置
# 最大昵称长度，默认15
# max_nickname_length=

# fund插件配置
# 是否启用基金查询插件，默认true
# fund_plugin_enabled=
# 获取历史数据天数（1-365），默认30天
# fund_history_days=
# 显示最近交易日天数（1-30），默认7天
# fund_display_recent_days=
# 缓存有效期（分钟，1-60），默认5分钟
# fund_cache_ttl_minutes=
# 最大缓存条目数（10-1000），默认100条
# fund_max_cache_size=
# 是否启用ETF查询，默认true
# fund_enable_etf=
# 是否启用LOF查询，默认true
# fund_enable_lof=
# 是否启用股票查询，默认true
# fund_enable_stocks=
# 是否启用场外基金查询，默认true
# fund_enable_off_market=
# 是否启用数据源切换（东方财富→同花顺），默认true
# fund_enable_data_source_fallback=
